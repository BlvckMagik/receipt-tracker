{
  "version": 2,
  "builds": [
    {
      "src": "apps/api/src/express-server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["apps/web/dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/api/src/express-server.ts"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build",
  "outputDirectory": "apps/web/dist",
  "framework": null
}
